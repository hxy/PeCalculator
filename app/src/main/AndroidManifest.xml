<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hy.pecalculator">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PEStartActivity" android:screenOrientation="landscape"/>

        <activity android:name=".PEMonthActivity" android:screenOrientation="landscape"/>
        <activity android:name=".PEChartMonthActivity" android:screenOrientation="landscape"/>

        <activity android:name=".PEMainActivity" android:screenOrientation="landscape"/>
        <activity android:name=".PEChartActivity" android:screenOrientation="landscape"/>



        <activity android:name=".PBMainActivity" android:screenOrientation="landscape"/>
        <activity android:name=".PBChartActivity" android:screenOrientation="landscape"/>
    </application>

</manifest>